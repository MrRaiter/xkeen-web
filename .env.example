# xkeen-web-ui Configuration
# Copy this file to .env and edit as needed

# Web server port
# Note: Port 90 is typically used by nfqws-keenetic-web
PORT=91

# xkeen configuration directory
XKEEN_CONFIG=/opt/etc/xkeen

# xkeen service script path
XKEEN_SERVICE=/opt/etc/init.d/S51xkeen

# Authentication credentials (CHANGE IN PRODUCTION!)
XKEEN_USER=root
XKEEN_PASS=keenetic
